<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
            <parameter key="mapbender.search.featuretype.service.id">features</parameter>
    </parameters>
    <services>

        <service class="Mapbender\SearchBundle\Component\QueryManager" id="mapbender.query.manager">
            <argument type="service" id="service_container"/>
            <argument type="service" id="%mapbender.search.featuretype.service.id%" />
        </service>
        <service class="Mapbender\SearchBundle\Component\StyleManager" id="mapbender.style.manager">
            <argument type="service" id="service_container"/>
        </service>

        <service class="Mapbender\SearchBundle\Component\StyleMapManager" id="mapbender.stylemap.manager">
            <argument type="service" id="service_container"/>
            <argument type="service" id="mapbender.style.manager"/>
        </service>
    </services>
</container>