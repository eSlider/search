<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <services>
        <!-- Services -->

        <service class="Mapbender\SearchBundle\Component\QueryManager" id="mapbender.query.manager">
            <argument type="service" id="service_container"/>
        </service>
        <service class="Mapbender\SearchBundle\Component\StyleManager" id="mapbender.style.manager">
            <argument type="service" id="service_container"/>
        </service>

        <service class="Mapbender\SearchBundle\Controller\StyleController" id="mapbender.style.controller">
            <argument type="service" id="service_container"/>
        </service>

        <service class="Mapbender\SearchBundle\Controller\QueryController" id="mapbender.query.controller">
            <argument type="service" id="service_container"/>
        </service>


    </services>
</container>